configuration {
    /* 
     * settings
     */

    eh: 1;
    lines: 5;
    width: 30;
    padding: 20;
    font: "Kiwi 7";
    scrollbar-width: 0;
    show-match: false;

    /*
     * colors
     */

    color-window: "#FDFDFD, #FDFDFD, #FDFDFD";
    color-normal: "#FDFDFD, #081A28, #FDFDFD, #FDFDFD, #F4ADB1";
    color-active: "#FDFDFD, #081A28, #FDFDFD, #FDFDFD, #F4ADB1";
    color-urgent: "#FDFDFD, #081A28, #FDFDFD, #FDFDFD, #F4ADB1";
}
