configuration {
    /* 
     * settings
     */

    eh: 1;
    lines: 5;
    width: 30;
    padding: 20;
    font: "Kiwi Medium 7";
    scrollbar-width: 0;
    show-match: false;

    /*
     * colors
     */

    color-window: "#EEEEEE, #EEEEEE, #EEEEEE";
    color-normal: "#EEEEEE, #102029, #EEEEEE, #EEEEEE, #F4ADB1";
    color-active: "#EEEEEE, #102029, #EEEEEE, #EEEEEE, #F4ADB1";
    color-urgent: "#EEEEEE, #102029, #EEEEEE, #EEEEEE, #F4ADB1";
}
