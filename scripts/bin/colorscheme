#!/usr/bin/env bash
#
# print colorscheme

# shellcheck disable=SC2034
f=3 b=4
for j in f b; do
    for i in {0..7}; do
        printf -v $j$i %b "\e[${!j}${i}m"
    done
done
t=$'\e[0m'
d=$'\e[1m'

# shellcheck disable=SC2154
cat << EOF

$f1██ $f2██ $f3██ $f4██ $f5██ $f6██ $f7██$t
$d$f1██ $f2██ $f3██ $f4██ $f5██ $f6██ $f7██$t

EOF
