! FONTS

! urxvt font
URxvt.font: -t-cherry-medium-r-normal--11-100-75-75-c-100-iso8859-1
URxvt.boldFont: -t-cherry-medium-r-normal--11-100-75-75-c-100-iso8859-1

! font rendering
Xft.antialias: 1
Xft.hinting: 1
Xft.hintstyle: hintslight
Xft.rgba: rgb
Xft.lcdfilter: lcddefault

! EXTENSIONS

! perl
URxvt.perl-ext-common: default,matcher,selection-to-clipboard

! urls
URxvt.url-launcher: chromium
URxvt.matcher.rend.0: fg13
URxvt.matcher.button: 3

